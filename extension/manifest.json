{
  "manifest_version": 2,
  "name": "YtShuffle Extension",
  "version": "1",
  "icons": {
    "16": "ytIcon/icon16.png",
    "48": "ytIcon/icon48.png",
    "128": "ytIcon/icon128.png"
  },
  "permissions":  [
    "activeTab",
    "declarativeContent"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_icon": {
      "19": "ytIcon/icon19.png",
      "38": "ytIcon/icon38.png"
    }
  }
}
